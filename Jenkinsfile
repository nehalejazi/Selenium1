pipeline {
    agent any

    stages {
        stage('Build 1') {
            steps {
                echo 'Hello World Build 1'
            }
        }
        stage('Test 1') {
            steps {
                echo 'Hello World Test 1'
            }
        }
        stage('Deploy 1') {
            steps {
                echo 'Hello World Deploy 1'
            }
        }
    }
}
